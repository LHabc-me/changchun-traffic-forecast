    "@fluentui/react-icons": "^2.0.224",
    "@fluentui/react-timepicker-compat": "^0.1.1",
    "@vitejs/plugin-react": "^4.0.4",
    "axios": "^1.6.2",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
